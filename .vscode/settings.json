{
  "breadcrumbs.enabled": false,

  "editor.cursorBlinking": "blink",
  "editor.fontFamily": "Consolas",
  "editor.fontLigatures": false,
  "editor.fontSize": 14,
  "editor.folding": false,
  "editor.formatOnSave": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "editor.mouseWheelScrollSensitivity": 3,
  "editor.mouseWheelZoom": true,
  "editor.tabSize": 4,
  "editor.wordWrapColumn": 180,

  "explorer.autoReveal": true,
  "explorer.decorations.colors": false,

  "files.autoSave": "onFocusChange",

  "git.autofetch": true,
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "gitHistory.showEditorTitleMenuBarIcons": false,

  "prettier.disableLanguages": [],
  "prettier.bracketSpacing": false,
  "prettier.printWidth": 180,
  "prettier.tabWidth": 4,
  "remote.WSL.fileWatcher.polling": true,
  
  "scm.diffDecorations": "none",

  "svgviewer.enableautopreview": false,
  "svgviewer.showzoominout": false,

  "typescript.updateImportsOnFileMove.enabled": "always",

  "vsicons.dontShowNewVersionMessage": true,
  "vsicons.presets.angular": true,
  "vsicons.projectDetection.autoReload": true,

  "workbench.colorTheme": "Default Dark+",
  "workbench.iconTheme": "vscode-icons",
  "json.format.enable": false,
  "vsicons.presets.hideExplorerArrows": true,
  "workbench.tree.indent": 18,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.editor.revealIfOpen": true,
  "ngschematics.schematics": ["@angular/material"],
  "outline.icons": false,
  "scm.providers.visible": 0,
  "terminal.integrated.copyOnSelection": true,
  "problems.decorations.enabled": true,
  "svgviewer.transparencycolor": "white",
  "svgviewer.transparencygrid": true,
  "workbench.tips.enabled": false
}
